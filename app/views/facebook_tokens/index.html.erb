<p id="notice"><%= notice %></p>

<h1>Facebook Tokens</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Encrypted token</th>
      <th>Encrypted token iv</th>
      <th>Network user</th>
      <th>Network user name</th>
      <th>Network user image url</th>
      <th>Expires at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @facebook_tokens.each do |facebook_token| %>
      <tr>
        <td><%= facebook_token.user %></td>
        <td><%= facebook_token.encrypted_token %></td>
        <td><%= facebook_token.encrypted_token_iv %></td>
        <td><%= facebook_token.network_user_id %></td>
        <td><%= facebook_token.network_user_name %></td>
        <td><%= facebook_token.network_user_image_url %></td>
        <td><%= facebook_token.expires_at %></td>
        <td><%= link_to 'Show', facebook_token %></td>
        <td><%= link_to 'Edit', edit_facebook_token_path(facebook_token) %></td>
        <td><%= link_to 'Destroy', facebook_token, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Facebook Token', new_facebook_token_path %>
