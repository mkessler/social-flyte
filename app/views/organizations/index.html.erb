<h2 class="dashboard-title">
  My Organizations
  <%= link_to new_organization_path, class: 'btn-floating btn-small green accent-4 mr-0' do %>
    <i class="fa fa-bank" aria-hidden="true" data-toggle="tooltip" data-placement="left" title="New Organization"></i>
  <% end %>
</h2>

<ul class="nav nav-tabs tabs-1 light-blue darken-2" role="tablist">
  <li class="nav-item">
    <a class="nav-link active" data-toggle="tab" href="#organizations-list" role="tab"><i class="fa fa-th-list" aria-hidden="true"></i> Organizations</a>
  </li>
</ul>

<div class="tab-content card">
  <div class="tab-pane fade in active" id="organizations-list" role="tabpanel">
    <%= render "organizations/datatable", organizations: @organizations %>
  </div>
</div>
