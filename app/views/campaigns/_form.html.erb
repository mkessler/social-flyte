<div class="row">
  <div class="col-lg-12">
    <%= simple_form_for([organization, campaign]) do |f| %>
      <div class="form-inputs">
        <%= f.input :name, icon: 'tag' %>
      </div>

      <div class="form-actions text-xs-center">
        <%= f.button :button do %>
          <i class="fa fa-check-circle-o left" aria-hidden="true"></i>
          Submit
        <% end %>
      </div>
    <% end %>
  </div>
</div>
